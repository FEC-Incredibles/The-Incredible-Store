    "babel-loader": "^8.2.2",
    "date-fns": "^2.30.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mongoose": "^6.1.4",
    "node-sass": "^4.9.3",
    "nodemon": "^2.0.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "webpack": "^5.41.0",
    "webpack-cli": "^4.7.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
